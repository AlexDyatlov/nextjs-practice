    "zod": "3.23.4"
  },
  "devDependencies": {
    "@types/node": "20",
    "@types/nodemailer": "6.4.14",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "clsx": "2.1.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.2.5",
    "sass": "1.74.1",
    "typescript": "5"
