{
  "name": "nextjs-practice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "WATCHPACK_POLLING=true next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "format": "prettier --write src",
    "ts-lint": "rimraf .next && tsc --noEmit --incremental",
    "control": "yarn ts-lint && yarn lint && yarn prettier --check src"
  },
  "dependencies": {
    "next": "14.1.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.2.5",
    "sass": "1.74.1",
    "typescript": "^5"
  }
}
